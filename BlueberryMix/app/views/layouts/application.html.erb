<!DOCTYPE html>
<html>
<head>
  <title>Blueberry Mix</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body>
<% if notice %>
  <p class="alert alert-success" style="color: blue;"><%= notice %></p>
<% end %>
<% if alert %>
  <p class="alert alert-danger" style="color: red"><%= alert %></p>
<% end %>
<%= yield %>

</body>
</html>
