{
  "plugins": [
    ["babel-plugin-styled-components", {
      "displayName": false,
      "ssr": true,
      "preprocess": false
    }],
    "babel-plugin-polished"
  ],
  "env": {
    "development": {
      "presets": "next/babel"
    },
    "production": {
      "presets": "next/babel"
    },
    "test": {
      "presets": [
        ["env", { "modules": "commonjs" }],
        "next/babel"
      ]
    }
  }
}
