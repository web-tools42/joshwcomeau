{
  "name": "willitbuild",
  "description": "Will it Build, a build time performance comparison of various CD solutions including Gatsby Cloud",
  "version": "0.0.1",
  "author": "Kyle Mathews <mathews.kyle@gmail.com>",
  "contributors": [
    "Dustin Schau <dustin@gatsbyjs.com>"
  ],
  "scripts": {
    "build": "gatsby build",
    "develop": "ENABLE_GATSBY_REFRESH_ENDPOINT=true LOCAL_DEV=1 gatsby develop",
    "refresh-content": "curl -X POST http://localhost:8000/__refresh",
    "lint": "prettier --write \"**/*.{js,jsx,json,md}\" && eslint -c .eslintrc src --fix --color --ext .js,.jsx,.ts,.tsx --report-unused-disable-directives --max-warnings=0",
    "start": "MOCK_MODE=true LOCAL_DEV=1 npm run develop",
    "start:build": "MOCK_MODE=true LOCAL_DEV=1 npm run build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "e2e:console": "cypress run",
    "e2e:ui": "cypress open",
    "e2e:ci": "cypress run --config baseUrl=http://localhost:9000",
    "audit:console": "yarn start:build && concurrently \"serve public -l 9000\" \"sleep 1 && cypress run --project audit --headless --browser chrome\"",
    "audit:ui": "yarn start:build && concurrently \"serve public -l 9000\" \"sleep 1 && cypress open --project audit\"",
    "audit:ci": "cypress run --project audit --headless --browser chrome"
  },
  "dependencies": {
    "@emotion/core": "^10.0.27",
    "@emotion/styled": "^10.0.27",
    "date-fns": "^2.9.0",
    "formik": "^2.1.4",
    "gatsby": "2.21.1",
    "gatsby-image": "^2.2.41",
    "gatsby-interface": "0.0.149",
    "gatsby-plugin-create-client-paths": "^2.1.22",
    "gatsby-plugin-emotion": "^4.1.22",
    "gatsby-plugin-google-analytics": "^2.3.2",
    "gatsby-plugin-manifest": "^2.2.39",
    "gatsby-plugin-netlify": "^2.2.1",
    "gatsby-plugin-offline": "^3.0.32",
    "gatsby-plugin-react-helmet": "^3.1.21",
    "gatsby-plugin-sharp": "^2.4.5",
    "gatsby-source-contentful": "^2.1.86",
    "gatsby-source-filesystem": "^2.1.46",
    "gatsby-source-graphql": "^2.1.33",
    "gatsby-transformer-json": "^2.2.28",
    "gatsby-transformer-sharp": "^2.3.14",
    "marked": "^0.8.0",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "react-icons": "^3.9.0",
    "recharts": "1.8.5",
    "theme-ui": "0.2.49",
    "typescript": "^3.8.2"
  },
  "devDependencies": {
    "@babel/core": "^7.8.6",
    "@storybook/addon-actions": "^5.3.14",
    "@storybook/addon-links": "^5.3.14",
    "@storybook/addons": "^5.3.14",
    "@storybook/react": "^5.3.14",
    "@typescript-eslint/eslint-plugin": "^2.21.0",
    "@typescript-eslint/parser": "^2.21.0",
    "babel-loader": "^8.0.6",
    "concurrently": "^5.1.0",
    "cypress": "^4.1.0",
    "cypress-audit": "^0.0.4",
    "eslint": "^6.8.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-config-react-app": "^5.2.0",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^2.5.0",
    "prettier": "^1.19.1",
    "serve": "^11.3.0"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsby-inc/willit.build"
  }
}
