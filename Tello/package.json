{
  "name": "tello",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start:client": "node scripts/start.js",
    "start:server": "NODE_ENV=development nodemon server/index.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom",
    "flow": "flow",
    "deploy": "NODE_ENV=production fly deploy:production",
    "deploy:no-build": "yarn deploy -- --skip-build",
    "restart:server": "fly restart:production",
    "stats": "NODE_ENV=production webpack --config config/webpack.config.prod.js --json > stats.json",
    "prettier": "prettier --single-quote --trailing-comma es5 --write \"src/**/*.js\""
  },
  "proxy": "http://localhost:3005",
  "dependencies": {
    "autoprefixer": "7.1.2",
    "aws-sdk": "^2.122.0",
    "babel-core": "6.25.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.1",
    "babel-plugin-emotion": "^8.0.10",
    "babel-preset-react-app": "^3.0.2",
    "babel-runtime": "6.23.0",
    "body-parser": "^1.17.2",
    "bundle-loader": "^0.5.5",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "compression": "^1.7.0",
    "cookie": "^0.3.1",
    "cookies-js": "^1.2.3",
    "css-loader": "0.28.4",
    "date-fns": "^1.28.5",
    "dotenv": "4.0.0",
    "emotion": "^8.0.10",
    "eslint": "4.4.1",
    "eslint-config-react-app": "^2.0.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.35.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.1.0",
    "express": "^4.15.4",
    "extract-text-webpack-plugin": "3.0.0",
    "file-loader": "0.11.2",
    "flightplan": "^0.6.17",
    "fs-extra": "3.0.1",
    "global": "^4.3.2",
    "html-webpack-plugin": "2.29.0",
    "immutability-helper": "^2.3.1",
    "jest": "20.0.4",
    "jsonwebtoken": "^7.4.3",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "mongodb": "^2.2.31",
    "mongoose": "^4.11.7",
    "nconf": "^0.8.4",
    "nodemon": "^1.11.0",
    "object-assign": "4.1.1",
    "passport": "^0.4.0",
    "passport-google-oauth": "^1.0.0",
    "passport-google-oauth2": "^0.1.6",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.6",
    "prettier": "^1.8.2",
    "promise": "8.0.1",
    "prop-types": "next",
    "react": "next",
    "react-confirm": "^0.1.15",
    "react-dev-utils": "^3.1.0",
    "react-dom": "next",
    "react-emotion": "^8.0.10",
    "react-error-overlay": "^1.0.10",
    "react-flip-move": "2.9.15",
    "react-icons": "^2.2.5",
    "react-lazyload": "^2.2.7",
    "react-motion": "^0.5.1",
    "react-redux": "^5.0.6",
    "react-router": "^4.1.2",
    "react-router-dom": "^4.1.2",
    "react-router-redux": "next",
    "react-swipe": "^5.0.8",
    "redux": "^3.7.2",
    "redux-devtools": "^3.4.0",
    "redux-devtools-dock-monitor": "^1.1.2",
    "redux-devtools-log-monitor": "^1.3.0",
    "request": "^2.82.0",
    "reselect": "^3.0.1",
    "slugify": "^1.2.1",
    "style-loader": "0.18.2",
    "sw-precache-webpack-plugin": "0.11.4",
    "swipe-js-iso": "^2.0.4",
    "url-loader": "0.5.9",
    "uuid": "^3.1.0",
    "webpack": "3.5.1",
    "webpack-dev-server": "2.7.1",
    "webpack-manifest-plugin": "1.2.1",
    "whatwg-fetch": "2.0.3"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  }
}
