{
  "name": "universal-config-browserify-example",
  "version": "0.0.0",
  "description": "Universal config browserify example",
  "main": "index.js",
  "scripts": {
    "build": "mkdir -p ./app/dist && ./node_modules/.bin/browserify ./app/client.js -t babelify -o ./app/dist/main.js",
    "server": "./node_modules/.bin/babel-node app/server.js",
    "start": "npm run build && npm run server"
  },
