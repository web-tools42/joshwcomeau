  "bugs": {
    "url": "https://github.com/naoufal/universal-config/issues"
  },
  "homepage": "https://github.com/naoufal/universal-config",
  "dependencies": {
    "babel": "5.8.21",
    "babelify": "6.2.0",
    "browserify": "11.0.1",
    "react": "0.13.3"
  }
}
